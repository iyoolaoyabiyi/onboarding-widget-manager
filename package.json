{
  "name": "my-onboarding-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run dev:web",
    "build": "pnpm run build:web && pnpm run build:widget",
    "lint": "pnpm run lint:web && pnpm run lint:widget",
    "dev:web": "pnpm run dev --filter web",
    "dev:widget": "pnpm run dev --filter widget",
    "build:web": "pnpm run build --filter web",
    "build:widget": "pnpm run build --filter widget",
    "preview:widget": "pnpm run preview --filter widget",
    "lint:web": "pnpm run lint --filter web",
    "lint:widget": "pnpm run lint --filter widget"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
